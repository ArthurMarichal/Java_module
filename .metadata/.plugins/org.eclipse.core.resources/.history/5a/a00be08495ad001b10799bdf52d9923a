package characters;

public class mage {

	private int life;
	private int attack;
	private String image;
	private String name;

	public mage() {
		super();
	}

	public mage(String name) {
		super();
		this.name = name;
	}

	public mage( String name, int life, int attack, String image) {
		super();
		this.life = life;
		this.attack = attack;
		this.image = image;
		this.name = name;
	}

	public String toString() {
		return name +" "+ life + " PV " + attack+ " PA " +image + " /n ";
	}
	public void setName(String name) { this.name = name; } 
	public String getName() { return name; } 
}