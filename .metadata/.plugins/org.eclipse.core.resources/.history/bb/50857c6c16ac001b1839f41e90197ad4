package characters;
public class war{
		
		private int life;
		private int attack;
		private String image;
		private String name ;
		
		public String getName() {
			
			return this.name ;
		}
		
		public void setName(String name) {
			
			this.name = name ;
	}	
		public String getNom() {
			
			return this.name ;
		}
		
		public void setNom(String name, int life, int attack, String image) {
			
			this.name = name ;
	}

		public war() {
			super();
		}

		public war(String name) {
			super();
			this.name = name;
		}

		public war(int life, int attack, String image, String name) {
			super();
			this.life = life;
			this.attack = attack;
			this.image = image;
			this.name = name;
		}
		
}
